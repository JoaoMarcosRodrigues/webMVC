@model IEnumerable<webMVC.Models.EmployeeModel>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "AddEmployee")
</p>
<table class="table">
    <tr>
        <th>
            ID
        </th>
        <th>
            EMPREGADO
        </th>
        <th>
            DEPARTAMENTO
        </th>
        <th>
            E-MAIL
        </th>
        <th>
            CELULAR
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EmpName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NameDep)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EmailId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MobileNo)
    </td>
    <td>
        @Html.ActionLink("Edit", "EditEmployee", new { id = item.Id }) |
        @Html.ActionLink("Delete", "DeleteEmployee", new { id = item.Id })
    </td>
</tr>
}

</table>
